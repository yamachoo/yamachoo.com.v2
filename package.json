{
  "name": "yamachoo.com.v2",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "TAILWIND_MODE=watch astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .astro,.js,.jsx,.ts,.tsx --ignore-path .gitignore --cache .",
    "lint:stylelint": "stylelint **/*.css --ignore-path .gitignore --cache",
    "lint:tsc": "tsc --noEmit",
    "lint:prettier": "prettier --ignore-path .gitignore --check .",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint --ext .astro,.js,.jsx,.ts,.tsx --ignore-path .gitignore --cache . --fix",
    "fix:stylelint": "stylelint **/*.css --ignore-path .gitignore --cache --fix",
    "fix:prettier": "prettier --ignore-path .gitignore --write ."
  },
  "lint-staged": {
    "*.{astro,js,jsx,ts,tsx}": "eslint --ignore-path .gitignore --cache --fix",
    "*.css": "stylelint --ignore-path .gitignore --cache --fix",
    "*": "prettier --ignore-path .gitignore --write --ignore-unknown"
  },
  "devDependencies": {
    "@astrojs/sitemap": "1.0.0",
    "@astrojs/tailwind": "2.1.3",
    "@tailwindcss/typography": "0.5.8",
    "@types/node": "18.11.10",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "astro": "1.6.12",
    "astro-compress": "1.1.15",
    "astro-seo": "0.6.0",
    "cssnano": "5.1.14",
    "daisyui": "2.42.1",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard-with-typescript": "23.0.0",
    "eslint-plugin-astro": "0.21.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.5.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-tailwindcss": "3.7.1",
    "husky": "8.0.2",
    "lint-staged": "13.0.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.0",
    "prettier-plugin-astro": "0.7.0",
    "prettier-plugin-md-nocjsp": "1.3.0",
    "prettier-plugin-tailwindcss": "0.2.0",
    "remark-code-titles": "0.1.2",
    "remark-external-links": "9.0.1",
    "stylelint": "14.16.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-standard": "29.0.0",
    "typescript": "4.9.3"
  }
}
