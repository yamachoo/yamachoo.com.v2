{
  "name": "@example/minimal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .astro,.js,.jsx,.ts,.tsx --ignore-path .gitignore --cache .",
    "lint:tsc": "tsc --noEmit",
    "lint:prettier": "prettier --ignore-path .gitignore --check .",
    "fix": "run-s fix:*",
    "fix:eslint": "npm run lint:eslint --fix",
    "fix:prettier": "prettier --ignore-path .gitignore --write ."
  },
  "lint-staged": {
    "*.{astro,js,jsx,ts,tsx}": "eslint --ignore-path .gitignore --cache --fix",
    "*": "prettier --ignore-path .gitignore --write --ignore-unknown"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "astro": "1.1.1",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard-with-typescript": "22.0.0",
    "eslint-plugin-astro": "0.17.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.2.5",
    "eslint-plugin-promise": "6.0.1",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-astro": "0.5.3",
    "typescript": "4.7.4"
  }
}
